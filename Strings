#include <iostream>
#include <string>
using namespace std;

int main() {
    string a;
    string b;
    cin>>a>>b;
    int len1,len2;
    len1=a.size();
    len2=b.size();
    cout<<len1<<" "<<len2;
    cout<<endl<<a+b<<endl;
    swap(a[0],b[0]);
    cout<<a<<" "<<b;

	// Complete the program
  
    return 0;
}
